{
    "name": "jupitern/slim4-skeleton",
    "description": "Slim starter / Slim skeleton package to boost your development with Slim framework",
    "keywords": [
        "slim", "slim php", "Slim skeleton", "Slim starter", "skeleton", "starter", "php framework", "api", "cli"
    ],
    "license": "MIT",
    "require": {
        "php": ">= 8.3",
        "jupitern/slimcore": "4.0.*",
        "monolog/monolog": "3.9.*",
        "league/flysystem": "3.29.*",
        "illuminate/database": "10.48.*",
        "predis/predis": "3.0.*",
        "league/plates": "3.6.*",
        "phpmailer/phpmailer": "6.10.*"
    },
    "require-dev": {
        "phpstan/phpstan": "2.1.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Lib\\": "lib/"
        },
        "files": [
            "lib/helpers.php"
        ]
    },
    "suggest": {
        "respect/validation": "1.1.*",
        "jupitern/table": "1.*",
        "slim/twig-view": "2.4.*"
    },
    "scripts": {
        "serve": "php -S localhost:8080 -t public"
    }
}
